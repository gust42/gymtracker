<div class="form-horizontal">

  <div  id="add-existing-exercise">
    <div class="form-group">
        <label for="inputEmail3" class="col-sm-2 control-label">Välj övning</label>

        <div class="col-sm-10">
            <select class="form-control" id="exercise-list"></select>
        </div>


    </div>
    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
            <button type="button" data-func="button_addexercise" class="btn btn-default">Lägg till</button>
        </div>
    </div>
  </div>
    <div id="create-new" class="hidden">
        <h4>Ny övning</h4>

        <div class="form-group">
            <label for="inputEmail3" class="col-sm-2 control-label">Namn på övning</label>

            <div class="col-sm-10">
                <input type="text" class="form-control" id="e-name">
            </div>
        </div>
        <div class="form-group">
            <label for="inputPassword3" class="col-xs-4 control-label">Repetetioner</label>

            <div class="col-xs-3">
                <input type="text" class="form-control" id="set" placeholder="Set">
            </div>
            <div class="col-xs-3">
                <input type="text" class="form-control" id="rep" placeholder="Rep">
            </div>
        </div>

        <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
                <button type="button" id="button-addexercise" data-func="button_addnewexercise" class="btn btn-default">Lägg
                    till
                </button>
                <button type="button" id="button-addexercise-cancel" data-func="button_newExercise" class="btn btn-default">Avbryt
                </button>
            </div>
        </div>
    </div>
</div>
